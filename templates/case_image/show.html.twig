{% extends 'base.html.twig' %}

{% block title %}CaseImage{% endblock %}

{% block body %}
    <h1>CaseImage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ case_image.id }}</td>
            </tr>
            <tr>
                <th>ImageUrl</th>
                <td>{{ case_image.imageUrl }}</td>
            </tr>
            <tr>
                <th>AltText</th>
                <td>{{ case_image.altText }}</td>
            </tr>
            <tr>
                <th>SortOrder</th>
                <td>{{ case_image.sortOrder }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ case_image.createdAt ? case_image.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_case_image_index') }}">back to list</a>

    <a href="{{ path('app_case_image_edit', {'id': case_image.id}) }}">edit</a>

    {{ include('case_image/_delete_form.html.twig') }}
{% endblock %}
